{
  "expo": {
    "runtimeVersion": "1.0.0",
    "name": "parent-notification",
    "slug": "parent-notification",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "jduapp",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "deeplinks": [
      {
        "pattern": "*",
        "fallback": "https:///appuri-hogosha.vercel.app/parentnotification"
      }
    ],
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jduapp.parentnotification",
      "infoPlist": {
        "CFBundleAllowMixedLocalizations": true
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.jduapp.parentnotification",
      "googleServicesFile": "./google-services.json",
      "intentFilters": [
        {
          "autoVerify": true,
          "action": "VIEW",
          "data": [
            {
              "scheme": "https",
              "host": "/appuri-hogosha.vercel.app",
              "pathPrefix": "/parentnotification",
              "pathPattern": ".*"

            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#000000"
        }
      ],
      "expo-router",
      "expo-font",
      "expo-localization"
    ],
    "experiments": {},
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "869d0b77-31b9-40cf-a27f-04ba0840dd3d"
      }
    },
    "owner": "raozas"
  }
}
