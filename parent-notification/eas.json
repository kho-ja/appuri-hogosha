{
	"cli": {
		"appVersionSource": "local",
		"requireCommit": true
	},
	"build": {
		"development": {
			"developmentClient": true,
			"distribution": "internal"
		},
		"preview": {
			"android": {
				"buildType": "apk",
				"gradleCommand": ":app:assembleRelease"
			},
			"developmentClient": true
		},
		"production": {
			"autoIncrement": false,
			"android": {
				"buildType": "apk"
			},
			"distribution": "internal",
			"environment": "production",
			"env": {
				"EXPO_PUBLIC_API_URL": "https://e45np4n3jb.execute-api.ap-northeast-1.amazonaws.com/api",
				"EXPO_PUBLIC_S3_BASE_URL": "https://parents-bucket.s3.ap-northeast-1.amazonaws.com/images"
			}
		}
	}
}
